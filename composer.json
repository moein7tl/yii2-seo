{
    "name": "moein7tl/yii2-seo",
    "description": "This package makes seo optimization more simple",
    "type": "library",
    "require": {
        "yiisoft/yii2": "^2.0"
    },
    "require-dev": {
      "yiisoft/yii2-codeception": "*",
      "yiisoft/yii2-faker": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Moein Hosseini Manesh",
            "email": "moein7tl@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "moein7tl\\yii2SEO\\": "src/"
        }
    },
    "minimum-stability": "dev",  "scripts":  {

    "post-package-install": [
        "@runMigrations"
    ],
    "post-package-update":  [
        "@runMigrations"
    ],
    "pre-package-uninstall":  [
        "@rollbackMigrations"
    ],
    "post-autoload-dump": [
        "@runMigrations"
    ],
    "runMigrations":  "php ./../../yii migrate/up --interactive --migrationPath=@app/vendor/moein7tl/yii2-seo/src/migrations",
    "rollbackMigrations": "php ./../../yii migrate/down 2 --interactive --migrationPath=@app/vendor/moein7tl/yii2-seo/src/migrations"

}
